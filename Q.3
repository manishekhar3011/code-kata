# Step 1: Input two floating-point numbers
float1 = float(input())
float2 = float(input())

# Step 2: Convert them to integers
int1 = int(float1)
int2 = int(float2)

# Step 3: Perform bitwise operations
bitwise_and = int1 & int2
bitwise_or = int1 | int2
bitwise_xor = int1 ^ int2
bitwise_not_and = (~int1) & int2

# Step 4: Output the results
print(f"Bitwise AND result: {bitwise_and}")
print(f"Bitwise OR result: {bitwise_or}")
print(f"Bitwise XOR result: {bitwise_xor}")
print(f"Bitwise NOT and AND result: {bitwise_not_and}")
